{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [2, "never"],
        "no-console": 0,
        "import/no-unresolved": [2, { "caseSensitive": false }],
        "object-curly-newline": [2, { "consistent": true }],
        "code": 120
    }
}
